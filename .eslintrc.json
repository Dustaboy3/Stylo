{
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module",
        "parser": "babel/eslint"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-undef": "off",
        "eqeqeq": "warn",
        "strict": "off"
    }
}